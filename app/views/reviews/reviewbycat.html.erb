  <head>
    <% @category.places.each do |p|%>
   <h1><%= p.user.name%>'s Reviews for the <%= p.category.name%> category</h1>
    <br><br>
    </head>
    <body>
    <%= p.name%> - <%= p.category.name%>
    <br><br><br>
    User <%= p.user.name%> - <%= p.description%> 
    <br>
    <%end%>
    <%= yield %>
    </body>

    